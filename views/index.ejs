    <h3>Minha Lista</h3>
    <div class="div3, wrapper">
      <section id="section1">
        <% FILMES.forEach(element => { %>
        <span class="item">

          <span class="cardTop">
            <a href="/editar/<%= element.id %> "><img class="icon"
                src="data:image/png;base64,"></a>
            <a href="/deletar/<%= element.id %> "><img class="icon"
                src="data:image/png;base64,"></a>
          </span>
          <span>
            <a href="/details/<%= element.id %> "><img class="image" src=' <%= element.img %>'></a>
            <div class="name"><%= element.nome %> </div>
          </span>
        </span>
        <% }) %>
    </div>
  </div>
